{
  "name": "wolkenkit-nevercompletedgame",
  "version": "1.0.0",
  "description": "wolkenkit-nevercompletedgame is a wolkenkit implementation of nevercompletedgame.com.",
  "wolkenkit": {
    "application": "ncg",
    "runtime": {
      "version": "3.1.0"
    },
    "environments": {
      "default": {
        "api": {
          "certificate": "/server/keys/localhost",
          "address": {
            "host": "localhost",
            "port": 3000
          },
          "allowAccessFrom": "*"
        },
        "node": {
          "environment": "development"
        },
        "fileStorage": {
          "allowAccessFrom": "*"
        }
      }
    }
  },
  "dependencies": {
    "wolkenkit-application-tools": "thenativeweb/wolkenkit-application-tools#2ec4cba0eac1491dfdbd2a210ab97b26c1877404"
  },
  "devDependencies": {
    "roboter": "13.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thenativeweb/wolkenkit-nevercompletedgame.git"
  },
  "keywords": [
    "wolkenkit"
  ],
  "license": "AGPL-3.0"
}
